{
    "name": "holo-mentor",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "pretty": "prettier --write \"./**/*.{ts,tsx,js,jsx,json,scss,css}\"",
        "configure-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\"",
        "docker:up": "docker compose -f docker-compose.yaml up -d",
        "docker:down": "docker compose -f docker-compose.yaml down -v"
    },
    "lint-staged": {
        "**/*.{ts,tsx,js,jsx,json,scss,css}": [
            "prettier --write ."
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "dependencies": {
        "@blocknote/core": "^0.14.5",
        "@blocknote/mantine": "^0.14.6",
        "@blocknote/react": "^0.14.6",
        "@loadable/component": "^5.16.4",
        "@nextui-org/react": "^2.4.2",
        "@nextui-org/table": "^2.0.36",
        "@reduxjs/toolkit": "^2.2.6",
        "@tailwindcss/forms": "^0.5.7",
        "@types/loadable__component": "^5.13.9",
        "axios": "^1.7.2",
        "chart.js": "^4.4.3",
        "cookies-next": "^4.2.1",
        "formik": "^2.4.6",
        "framer-motion": "^11.2.12",
        "jwt-decode": "^4.0.0",
        "lodash": "^4.17.21",
        "moment": "^2.30.1",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-chartjs-2": "^5.2.0",
        "react-countup": "^6.5.3",
        "react-dom": "^18.2.0",
        "react-redux": "^9.1.2",
        "react-router-dom": "^6.23.1",
        "react-tailwindcss-select": "^1.8.5",
        "yup": "^1.4.0"
    },
    "devDependencies": {
        "@types/node": "^20.14.9",
        "@types/react": "^18.2.66",
        "@types/react-dom": "^18.2.22",
        "@typescript-eslint/eslint-plugin": "^7.2.0",
        "@typescript-eslint/parser": "^7.2.0",
        "@vitejs/plugin-react-swc": "^3.5.0",
        "autoprefixer": "^10.4.19",
        "babel-plugin-root-import": "^6.6.0",
        "eslint": "^8.57.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.6",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.3",
        "postcss": "^8.4.38",
        "prettier": "^2.7.1",
        "sass": "^1.77.4",
        "tailwindcss": "^3.4.3",
        "typescript": "^5.2.2",
        "vite": "^5.3.5",
        "vite-tsconfig-paths": "^4.3.2"
    }
}
